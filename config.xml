<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="2" id="io.ionic.ebc" version="2.0.0" windows-packageVersion="2.0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>EBC v2</name>
    <description>An Ionic Framework and Cordova project. EBC's cards and Fylers.</description>
    <author email="hi@ionicframework" href="http://ionicframework.com/">EBC: Electrionic Business Card</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <plugin name="cordova-plugin-camera" version="2.1.1" />
    <plugin name="cordova-plugin-contacts" version="2.0.1" />
    <plugin name="cordova-plugin-file" version="4.1.1" />
    <plugin name="cordova-plugin-dialogs" version="1.2.0" />
    <plugin name="cordova-plugin-inappbrowser" version="1.3.0" />
    <plugin name="cordova-plugin-file-transfer" version="1.5.0" />
    <plugin name="cordova-plugin-media-capture" version="1.2.0" />
    <plugin name="cordova-plugin-media" version="2.2.0" />
    <plugin name="cordova-plugin-x-toast" src="https://github.com/EddyVerbruggen/Toast-PhoneGap-Plugin.git" version="2.5.0" />
    <preference name="WindowsStoreDisplayName" value="EBC: Electronic Business Card" />
    <vs:platformSpecificValues xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
        <vs:platformSpecificWidget id="io.ionic.ebc" platformName="windows" version="2.0.0.0" />
    </vs:platformSpecificValues>
    <preference name="android-maxSdkVersion" value="14" />
    <preference name="android-targetSdkVersion" value="15" />
    <plugin name="cordova-plugin-device" spec="~1.1.1" />
    <plugin name="cordova-plugin-console" spec="~1.0.3" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.1" />
    <plugin name="cordova-plugin-splashscreen" spec="~3.2.1" />
    <plugin name="cordova-plugin-statusbar" spec="~2.1.2" />
    <plugin name="ionic-plugin-keyboard" spec="https://github.com/driftyco/ionic-plugin-keyboard.git" />
</widget>
